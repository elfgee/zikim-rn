{
  "name": "zikim-rn-workspace",
  "packageManager": "yarn@1.22.22",
  "private": true,
  "scripts": {
    "build:zuix": "cd packages/zuix && yarn build",
    "dev:app": "cd apps/zikim-rn && npx expo start",
    "dev": "yarn build:zuix && yarn dev:app",
    "vercel:build": "yarn build:zuix && cd apps/zikim-rn && npx expo export --platform web"
  },
  "workspaces": ["apps/*", "packages/*"],
  "resolutions": {
    "react-native-web": "0.21.2"
  }
}
